<div class="container">
<p id="notice"><%= notice %></p>
<h2>Listing Deliveries</h2>

<div class="well">
  <%= link_to 'New Delivery', new_delivery_path, remote: true, class: "btn btn-primary" %>
</div>

<table id class="table table-bordered table-striped">
  <thead>
    <tr>
      <th>Direction</th>
      <th>Cost</th>
      <th>Execution document</th>
      <th>Check factory</th>
      <th>Bank service</th>
      <th>Bank percent</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody id="deliveries">
    <%= render @deliveries %>
  </tbody>
</table>
<%= render 'dialog' %>
</div>
