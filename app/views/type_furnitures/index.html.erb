<div class="container"> 
  <p id="notice"><%= notice %></p>

  <h2><%= t('type_furnitures.listing') %></h2>
  <div class="well">
    <%= link_to t('type_furnitures.new'), new_type_furniture_path, remote: true, class: "btn btn-primary" %>
  </div>
  <div class="new-type_furniture"></div>
  <table class="table table-bordered table-striped table-hover">
    <thead>
      <tr>
        <th><%= t('type_furnitures.name') %></th>
        <th colspan="3"></th>
      </tr>
    </thead>

    <tbody id="type_furnitures">
      <%= render @type_furnitures %>
    </tbody>
  </table>
  <%= render 'dialog' %>
</div>